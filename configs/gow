[gow]
  test          bin/xargs.exe
  page.rls      https://github.com/bmatzelle/gow/releases
  url.get       rls
  url.rx        (/bmatzelle/gow/releases/download/v.*?/Gow-.*?.exe)
  url.prepend   https://github.com
  name.get      _url
  name.rx       /download/v.*?/(Gow-.*?.exe)
  folder.get    _name
  folder.rx     (Gow-.*?).exe
  invoke        @FILE@ /S /D=@DEST@
  path          bin
