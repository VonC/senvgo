[gvim]
  test			   gvim.exe
  page.gvim    http://ftp.vim.org/pub/vim/pc/?C=M;O=D
  url.get      gvim
  url.rx       href="(gvim[\d_]+\.zip)"
  url.prepend  http://ftp.vim.org/pub/vim/pc/
  name.get     _url
  name.rx      (gvim[\d_]+\.zip)
  folder.get   _name
  folder.rx    (gvim[\d_]+\.zip)
  doskey       vi=~gvim.exe $*
